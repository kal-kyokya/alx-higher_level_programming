#!/usr/bin/python3
import random
number = random.randint(-10000, 10000)
divisor = 10
if (number < 0):
	number *= -1
while (number / divisor > 9):
	divisor = divisor * 10
remainder = number % divisor
print("Last digit of {} is {}".format(number, remainder), end = ' ')
if (remainder > 5):
	print("and is greater than 5")
elif (remainder == 0):
	print("and is 0")
else:
	print("and is less than 6 and not 0")
